
material fireball
{
	technique
	{
		pass
		{
			ambient 0 0 0 1
			lighting off

			texture_unit
			{
				texture textures/fireball.png
			}
		}

	}

}



material flare
{
	technique
	{
		pass
		{
			ambient 0 0 0 1
			scene_blend alpha_blend
			lighting off

			texture_unit
			{
				texture textures/flares.png
			}
		}

	}

}


